{
  "name": "token-liquidity",
  "version": "3.0.0",
  "description": "BCH SLP Token Liquidity Web App for the Permissionless Software Foundation",
  "main": "token-liquidity.js",
  "scripts": {
    "start": "NETWORK=mainnet node token-liquidity.js",
    "test": "export TL_ENV=test && npm run prep-test && NETWORK=testnet  TEST_ENV=unit nyc --reporter=text mocha --exit --timeout 15000 test/unit",
    "test:integration": "export TL_ENV=test && npm run prep-test && NETWORK=testnet TEST_ENV=integration nyc --reporter=text mocha --exit --timeout 30000 test/unit",
    "test:temp": "mocha -g '#transactions' test/unit",
    "lint": "standard --env mocha --fix",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs",
    "coverage": "export TL_ENV=test && npm run prep-test && nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "export TL_ENV=test && npm run prep-test && nyc --reporter=html mocha --timeout 15000 --exit test/unit",
    "prep-test": "node util/users/delete-all-test-users.js"
  },
  "keywords": [
    "koa-api-boilerplate",
    "api",
    "koa",
    "koa2",
    "boilerplate",
    "es6",
    "mongoose",
    "passportjs",
    "apidoc",
    "bitcoin-cash",
    "bitcoin",
    "BCH",
    "wormhole"
  ],
  "author": "Chris Troutner <chris.troutner@gmail.com>",
  "license": "MIT",
  "apidoc": {
    "title": "token-liquidity",
    "url": "localhost:5000"
  },
  "repository": "Permissionless-Software-Foundation/token-liquidity",
  "dependencies": {
    "@psf/bch-js": "3.10.3",
    "apidoc": "^0.23.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bitbox-sdk": "^8.8.0",
    "collect.js": "^4.16.6",
    "glob": "^7.0.0",
    "got": "^9.5.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-bch-lib": "^1.2.0",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^2.0.1",
    "koa-logger": "^3.1.0",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "line-reader": "^0.4.0",
    "mongoose": "^5.10.15",
    "nodemailer": "^6.4.10",
    "p-queue": "^6.2.1",
    "p-retry": "^4.2.0",
    "passport-local": "^1.0.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.3",
    "slp-sdk": "^4.11.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.0.0"
  },
  "devDependencies": {
    "ajv": "^5.5.2",
    "bitbox-mock": "^1.0.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.2.5",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^7.0.1",
    "nock": "^10.0.6",
    "nyc": "^15.0.0",
    "semantic-release": "^17.2.3",
    "sinon": "^9.2.1",
    "standard": "^14.3.1"
  },
  "release": {
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": false
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
